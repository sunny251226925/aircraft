}
.font-size-20{
  font-size: 20px;
}
.font-size-12{
  font-size: 12px;
}
.bg-color-lan{
  background-color: #1A67DE;
}
.bg-color-white{
  background-color: #fff;
}
.text-left{
  text-align: left;
}
.text-center{
  text-align: center;
}
.text-right{
  text-align: right;
}
.logo{
  padding-top: 35px;
  text-align: center;
}
.logo image{
  width: 130px;
  height: 130px;
}
.logo text{
  display: block;
  color: #fff;
  margin-top: 20px;
  font-size: 20px;
}
.duihao{
  width:17px;
  height:17px;
  line-height: 17px;
  vertical-align: middle;
  margin-right: 5px;
}
.getQrCode{
  position: absolute;
  top: 3px;
  right: 3px;
  height: calc(100% - 6px);
  line-height: 31px;
  display: inline-block;
  border-radius: 20px;
  padding: 0 15px;
  color: #fff;
  background-color: #1A67DE;
  z-index: 2;
}


.container {
  display: flex;
  width: 100vw;
  height: 100vh;
  box-sizing: border-box;
  flex-direction: column;
}

.header{
  position: relative;
  display: flex;
  width: 100%;
  height: 40px;
  line-height: 40px;
  color: #fff;
}
.header .item{
  flex: 1;
  height: 100%;
  text-align: center;
  box-sizing: border-box;
  color: #fff;
}
.header .item:first-child{
  text-align: left;
  padding-left: 15px;
  box-sizing: border-box;
}
.header .item:last-child{
  text-align: right;
  padding-right: 15px;
  box-sizing: border-box;
}
.header .pageBack{
  width: 10px;
  height: 18px;
}

.box{
  position: relative;
  flex: 1;
  width: 100vw;
  padding-top: 15px;
  box-sizing: border-box;
  box-shadow: 0px -1px 0 #909090 inset;
}
.box.white{
  background-color: white;
}
.box.padding{
  padding: 15px;
}
.box-left{
  float: left;
  width: calc(100% - 40px);
}
.box-right{
  float: left;
  width: 40px;
}
.ellipsis{
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}

.row{
  position: relative;
  display: flex;
  padding: 0 15px;
  width: 100%;
  margin-bottom: 15px;
  flex-wrap: wrap;
  box-sizing: border-box;
}
.row-auto{
  position: relative;
  display: flex;
  width: 100%;
  box-sizing: border-box;
}
.col-1{
  flex: 1;
}
.col-2{
  flex: 2;
}
.col-3{
  flex: 3;
}
.col-4{
  flex: 4;
}
.col-5{
  flex: 5;
}
.col-6{
  flex: 6;
}
.col-7{
  flex: 7;
}
.col-8{
  flex: 8;
}
.col-9{
  flex: 9;
}
.col-1-wrap{
  flex: 0 0 50%;
}
.col-2-wrap{
  flex: 0 0 100%;
}
.col-last{
  width: 35px;
  height: 37px;
  line-height: 37px;
  text-align: right;
}
.col-last.max{
  width: 100px;
  height: 37px;
  line-height: 37px;
  text-align: right;
}
[class*='col']{
  position: relative;
}

.title-1{
  text-align: left;
  line-height: 20px;
  color: #fff;
}
.title-2{
  text-align: right;
  line-height: 20px;
  color: #fff;
  padding-right: 15px;
}
.title-3{
  text-align: center;
  line-height: 20px;
  color: #fff;
}


.title-1::before{
  content: "‚óè";
  color: #fff;
  margin-right: 10px;
}
.title-1.none::before{
  content: "";
}
.underline{
  text-decoration:underline;
}


.img-22{
  width: 22px;
  height: 22px;
  margin-bottom: 0 !important;
}
.jiantou-zuo{
  width: 8px;
  height: 22px;
  vertical-align: middle;
}

.b-bottom-1{
  border-bottom: 1px solid rgba(247,247,247,1);
}

.input-1{
  position: relative;
  width: 100%;
  height: 37px;
  color: #979797;
  padding: 0 15px;
