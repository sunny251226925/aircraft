.link-list{
  position: relative;
  padding: 10px 20px;
  background-color: #fff;
  border-radius: 5px;
  margin-bottom: 15px;
}
.link-list .link{
  position: relative;
  display: flex;
  width: 100%;
  height: 20px;
  line-height: 20px;
  border-bottom: 1px solid rgba(233,233,233,1);
  padding-bottom: 10px;
  margin-bottom: 10px;
}
.link-list .link:last-child{
  border-bottom: 0;
  padding-bottom: 0;
  margin-bottom: 0;
}
.link-list .link .link-left{
  flex: 1;
  text-align: left;
  height: 100%;
  color: #4A4A4A;
}
.link-list .link .link-right{
  width: 50px;
  text-align: right;
  height: 100%;
  font-size: 12px;
  color: #A4A4A4;
}
.link-list .link .link-right image{
  width: 10px;
  height: 18px;
}

.row-auto .link-tag{
  height: 80px;
  text-align: center;
}
.row-auto .link-tag .title{
  line-height: 27px;
  color: #666;
}
.row-auto image{
  width: 43px;
  height: 43px;
