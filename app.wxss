}

.eye{
  position: absolute;
  width: 22px;
  height: 14px;
  top: 50%;
  right: 15px;
  transform: translate(0,-50%);
  z-index: 2;
}

.box-shadow-lan{
  -webkit-box-shadow:0 0 10px #1A67DE;
  -moz-box-shadow:0 0 10px #1A67DE;
  box-shadow:0 0 10px #1A67DE;
}

.box-shadow-hui{
  -webkit-box-shadow:0 0 10px #666;
  -moz-box-shadow:0 0 10px #666;
  box-shadow:0 0 10px #666;
}

.list-box .head{
  width: 100%;
  height: 50px;
  background-color: #1A67DE;
}
.list-box .list{
  position: relative;
  width: 100%;
  padding: 33px 15px;
  box-sizing: border-box;
}

.list-box .list-item{
  position: relative;
  width: 100%;
  padding: 15px 10px;
  border-radius: 5px;
  margin-bottom: 10px;
  box-sizing: border-box;
  overflow: hidden;
}

.list-box .list-item .content{
  display: flex;
  width: 100%;
  height: 48px;
}
.list-box .list-item .content .left{
  width: 48px;
  height: 48px;
}
.list-box .list-item .content .center{
  flex: 1;
  height: 48px;
  padding-left: 10px;
  box-sizing: border-box;
}
.list-box .list-item .content .right{
  width: 20px;
  height: 48px;
  text-align: center;
  line-height: 48px;
}
.list-box .list-item .content .right image{
  width: 8px;
  height: 20px;
  vertical-align: middle;
}

.center-style-1{
  padding-top: 8px;
}

.box-radius{
  width: 48px;
